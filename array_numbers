public static void printCombinationsThatAddUpToTen(int[] arr) {
    int n = arr.length;
    for (int i = 0; i < n; i++) {
        for (int j = i + 1; j < n; j++) {
            if (arr[i] + arr[j] == 10) {
                System.out.println(arr[i] + " + " + arr[j] + " = 10");
            }
        }
    }
}

int[] arr = { 1, 2, 3, 4, 5, 6, 7, 8, 9 };
printCombinationsThatAddUpToTen(arr);
